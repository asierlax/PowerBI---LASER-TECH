	partition Articulos-23e52532-cf86-420a-9324-3147ff6164b4 = m
		mode: import
		source =
				let
				    CurrentServer = Instancia,
				    CurrentDatabase = BaseDeDatos,
				    CurrentSchema = Esquema,
				    CurrentCompany = Empresa,
				    Origen = Sql.Database(CurrentServer, CurrentDatabase, [Query="
				                                                                SELECT ITM.ITMREF_0 AS 'CodArticulo',
				                                                                ITM.TCLCOD_0 AS 'IdCategoria',
				                                                                TXCAT.TEXTE_0 AS 'Categoria',
				                                                                ITM.ACCCOD_0 AS 'Codigo Contable',
				                                                                ITM.ITMDES1_0 AS 'Descripcion 1',
				                                                                ITM.ITMDES2_0 AS 'Descripcion 2',
				                                                                ITM.ITMDES3_0 AS 'Descripcion 3',
				                                                                ITM.STU_0 AS 'Unidad',
				                                                                ITM.VACITM_0 AS 'Nivel Impuesto',
				                                                                ITM.BUY_0 AS 'Comprador',
				                                                                ITM.PURBASPRI_0 AS 'Precio Base',
				                                                                ITM.CPRCOE_0 AS 'Coef Gastos Entrega',
				                                                                ITM.CPRAMT_0 AS 'Coste Fijo Unitario',
				                                                                ITM.CCE_0 AS 'Eje Analitico 1',
				                                                                ITM.CCE_1 AS 'Eje Analitico 2',
				                                                                ITM.CCE_2 AS 'Eje Analitico 3',
				                                                                ITM.CCE_3 AS 'Eje Analitico 4',
				                                                                ITM.TSICOD_0 AS 'Codigo Famlia',
				                                                                ITM.TSICOD_1 AS 'Codigo Subfamilia',
				                                                                ATXFAM.TEXTE_0 AS 'Familia',
				                                                                ATXFAM2.TEXTE_0 AS 'Subfamilia',
				                                                                ITMS.MINQTY_0 AS 'Cantidad Minima',
				                                                                ITMS.MINPFM_0 AS 'Margen Minimo',
				                                                                ITMS.MINPRI_0 AS 'Precio Minimo',
				                                                                ITMS.BASPRI_0 AS 'Precio Base Ventas'
				
				                                                                FROM " & CurrentSchema & ".ITMMASTER ITM
				                                                                LEFT JOIN " & CurrentSchema & ".ITMSALES ITMS ON ITM.ITMREF_0 = ITMS.ITMREF_0
				                                                                LEFT JOIN " & CurrentSchema & ".ATEXTRA ATXFAM ON ITM.TSICOD_0 = ATXFAM.IDENT2_0  AND ATXFAM.CODFIC_0 = 'ATABDIV' AND ATXFAM.ZONE_0 = 'LNGDES' AND ATXFAM.IDENT1_0 = '20' AND ATXFAM.LANGUE_0 = 'SPA'
				                                                                LEFT JOIN " & CurrentSchema & ".ATEXTRA ATXFAM2 ON ITM.TSICOD_1 = ATXFAM2.IDENT2_0 AND ATXFAM2.CODFIC_0 = 'ATABDIV' AND ATXFAM2.ZONE_0 = 'LNGDES' AND ATXFAM2.IDENT1_0 = '21' AND ATXFAM2.LANGUE_0 = 'SPA'
				                                                                LEFT JOIN " & CurrentSchema & ".ATEXTRA TXCAT ON ITM.TCLCOD_0 = TXCAT.IDENT1_0 AND TXCAT.CODFIC_0 = 'ITMCATEG' AND TXCAT.ZONE_0 = 'TCLAXX' AND TXCAT.LANGUE_0 = 'SPA'
				
				                                                          "])
				in
				    Origen

	annotation PBI_ResultType = Table

	annotation PBI_NavigationStepName = Navigation

