	partition 'Pedidos Lineas' = m
		mode: import
		source = ```
				let
				    CurrentServer = Instancia,
				    CurrentDatabase = BaseDeDatos,
				    CurrentSchema = Esquema,
				    Origen = Sql.Database(CurrentServer, CurrentDatabase, [Query="
				                                                                SELECT
				                                                                    POQ.POHNUM_0 AS 'Documento',
				                                                                    POQ.POPLIN_0 AS 'IdLinea',
				                                                                    POQ.ITMREF_0 AS 'IdArticulo',
				                                                                    POQ.ITMREFBPS_0 AS 'Ref. proveedor',
				                                                                    POP.ITMDES_0 AS 'Descripcion linea',
				                                                                    POQ.PRHFCY_0 AS 'Planta',
				                                                                    POQ.QTYUOM_0 AS 'Ctd. pedida',
				                                                                    POQ.UOM_0 AS 'Uni.',
				                                                                    POQ.UOMPUUCOE_0 AS 'Coef. UC-US',
				                                                                    POQ.QTYSTU_0 AS 'Ctd. US',
				                                                                    POQ.STU_0 AS 'Uni. stock',
				                                                                    POQ.EXTRCPDAT_0 AS 'Fecha recepcion',
				                                                                    POQ.XFECHAOCC_0 AS 'Fecha OCC', 
				                                                                    POP.GROPRI_0 AS 'Precio',
				                                                                    POP.DISCRGVAL1_0 AS 'Dto1',
				                                                                    POP.DISCRGVAL2_0 AS 'Dto2',
				                                                                    POP.DISCRGVAL3_0 AS 'Dto3',
				                                                                    POP.NETPRI_0 AS 'Precio neto',
				                                                                    (POQ.QTYUOM_0 * POP.NETPRI_0) AS 'Importe linea',
				                                                                    CASE POQ.LINCLEFLG_0 WHEN 2 THEN 'Si' ELSE 'No' END AS 'Linea saldada'
				                                                                FROM " & CurrentSchema & ".PORDERQ POQ
				                                                                INNER JOIN " & CurrentSchema & ".PORDERP POP ON POQ.POHNUM_0 = POP.POHNUM_0 AND POQ.POPLIN_0 = POP.POPLIN_0
				                                                          "]),
				    #"Tipo cambiado" = Table.TransformColumnTypes(Origen,{{"Fecha recepcion", type date}, {"Fecha OCC", type date}})
				in
				    #"Tipo cambiado"
				```

